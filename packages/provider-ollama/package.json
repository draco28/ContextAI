{
  "name": "@contextaisdk/provider-ollama",
  "version": "0.1.0",
  "description": "Ollama local LLM provider for ContextAI SDK",
  "license": "MIT",
  "author": "ContextAI Team",
  "homepage": "https://github.com/draco28/contextai#readme",
  "bugs": {
    "url": "https://github.com/draco28/contextai/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/draco28/contextai.git",
    "directory": "packages/provider-ollama"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@contextaisdk/core": "workspace:*"
  },
  "devDependencies": {
    "@contextaisdk/tsconfig": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.1.0"
  },
  "keywords": [
    "contextai",
    "ollama",
    "llm",
    "ai",
    "provider",
    "local"
  ],
  "publishConfig": {
    "access": "public"
  }
}
