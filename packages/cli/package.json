{
  "name": "@contextai/cli",
  "version": "0.0.1",
  "description": "CLI for scaffolding ContextAI projects and packages",
  "type": "module",
  "bin": {
    "contextai": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "ejs": "^3.1.10",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.3.7",
    "ora": "^8.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@contextai/tsconfig": "workspace:*",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.0",
    "tsup": "^8.3.0",
    "typescript": "^5.5.0",
    "vitest": "^2.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "contextai",
    "cli",
    "scaffolding",
    "generator",
    "monorepo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/contextai/contextai.git",
    "directory": "packages/cli"
  },
  "license": "MIT"
}
