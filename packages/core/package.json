{
  "name": "@contextai/core",
  "version": "0.0.1",
  "description": "ContextAI Agent SDK - TypeScript-first AI Agent runtime with ReAct reasoning",
  "license": "MIT",
  "author": "ContextAI Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/draco28/ContextAI.git",
    "directory": "packages/core"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "react",
    "reasoning",
    "typescript",
    "sdk"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@contextai/tsconfig": "workspace:*",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.5.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "zod": "^3.23.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
