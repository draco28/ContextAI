{
  "name": "@contextai/provider-openai",
  "version": "0.0.1",
  "description": "OpenAI GPT provider for ContextAI SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@contextai/core": "workspace:*"
  },
  "peerDependencies": {
    "openai": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": false
    }
  },
  "devDependencies": {
    "@contextai/tsconfig": "workspace:*",
    "openai": "^4.77.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.1.0"
  },
  "keywords": [
    "openai",
    "gpt",
    "llm",
    "ai",
    "contextai",
    "provider"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
